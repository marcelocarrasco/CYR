    <sql>SELECT &#x27;&#x24;&#x7b;CYR-ROOT&#x7d;&#x2f;sqlcl&#x2f;bin&#x2f;sql -s harriague&#x2f;HARRIAGUE&#x40;WILLY.WORLD &#x40;&#x24;&#x7b;CYR-HOME&#x7d;&#x2f;Scripts&#x2f;status_umts_gral.sql &#x27;&#xa;  &#x7c;&#x7c;TO_CHAR&#x28;sysdate-1,&#x27;dd.mm.yyyy&#x27;&#x29;&#xa;  &#x7c;&#x7c;&#x27; &#x27;&#xa;  &#x7c;&#x7c;TO_CHAR&#x28;sysdate-1,&#x27;dd.mm.yyyy&#x27;&#x29;&#xa;  &#x7c;&#x7c;&#x27; &#x27;&#xa;  &#x7c;&#x7c; ossrc&#xa;  &#x7c;&#x7c;&#x27; &#x27;&#x7c;&#x7c;&#x27;&#x27;&#x27;&#x27;&#xa;  &#x7c;&#x7c;&#x24;&#x7b;WCELL-SRL-PCT&#x7d;&#x7c;&#x7c;CHR&#x28;44&#x29;&#xa;  &#x7c;&#x7c;&#x24;&#x7b;WCELL-TRF-PCT&#x7d;&#x7c;&#x7c;CHR&#x28;44&#x29;&#xa;  &#x7c;&#x7c;&#x24;&#x7b;WCELL-CRS-PCT&#x7d;&#x7c;&#x7c;CHR&#x28;44&#x29;&#xa;  &#x7c;&#x7c;&#x24;&#x7b;WCELL-HSW-PCT&#x7d;&#x7c;&#x7c;CHR&#x28;44&#x29;&#xa;  &#x7c;&#x7c;&#x24;&#x7b;WCELL-CTP-PCT&#x7d;&#x7c;&#x7c;CHR&#x28;44&#x29;&#xa;  &#x7c;&#x7c;&#x24;&#x7b;WCELL-RRC-PCT&#x7d;&#x7c;&#x7c;CHR&#x28;44&#x29;&#xa;  &#x7c;&#x7c;&#x24;&#x7b;WCELL-YHO-PCT&#x7d;&#x7c;&#x7c;CHR&#x28;44&#x29;&#xa;  &#x7c;&#x7c;&#x24;&#x7b;WCELL-SHO-PCT&#x7d;&#x7c;&#x7c;CHR&#x28;44&#x29;&#xa;  &#x7c;&#x7c;&#x24;&#x7b;WCELL-IHO-PCT&#x7d;&#x7c;&#x7c;CHR&#x28;44&#x29;&#xa;  &#x7c;&#x7c;&#x24;&#x7b;WCELL-CTP-PCT&#x7d;&#x7c;&#x7c;CHR&#x28;44&#x29;&#xa;  &#x7c;&#x7c;&#x24;&#x7b;WCELL-CPI-PCT&#x7d;&#x7c;&#x7c;CHR&#x28;44&#x29;&#xa;  &#x7c;&#x7c;&#x24;&#x7b;WCELL-L3I-PCT&#x7d;&#x7c;&#x7c;CHR&#x28;44&#x29;&#xa;  &#x7c;&#x7c;&#x24;&#x7b;WCELL-PKT-PCT&#x7d;&#xa;  &#x7c;&#x7c;&#x27;&#x27;&#x27;&#x27;&#xa;  &#x7c;&#x7c;&#x27; &#x27;&#xa;  &#x7c;&#x7c;&#x27;&#x24;&#x7b;CYR-DATA-DIRECTORY&#x7d;&#x2f;status_umts_gral_&#x27;&#x7c;&#x7c;ossrc&#x7c;&#x7c;&#x27;.html&#x27; as query,&#xa;  &#x27;sh &#x24;&#x7b;CYR-HOME&#x7d;&#x2f;Scripts&#x2f;replace_char_in_file.sh &#x24;&#x7b;CYR-DATA-DIRECTORY&#x7d;&#x2f;status_umts_gral_&#x27;&#x7c;&#x7c;ossrc&#x7c;&#x7c;&#x27;.html &#x24;&#x7b;CYR-DATA-DIRECTORY&#x7d;&#x2f;status_umts_table_&#x27;&#x7c;&#x7c;ossrc&#x7c;&#x7c;&#x27;.html&#x27; as sed_script&#xa;FROM&#xa;  &#x28;SELECT ELEMENT_CLASS,&#xa;    OSSRC,&#xa;    COUNT&#x28;DISTINCT element_name&#x29; AS CANTIDAD&#xa;  FROM MULTIVENDOR_OBJECT2&#xa;  WHERE ELEMENT_CLASS  IN &#x28;&#x27;WCELL&#x27;&#x29;&#xa;  AND VALID_FINISH_DATE &#x3e; SYSDATE&#xa;  GROUP BY ossrc,&#xa;    ELEMENT_CLASS&#xa;  &#x29;</sql>
